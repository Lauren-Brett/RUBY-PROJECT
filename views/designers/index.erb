

<div class="heading-for-page">
  <h1>Listings of current Designers</h1>
  <hr>
</div>

<div class="container">

<table>
<tr>
  <th>Company Name</th>
  <th></th>
  <th>Company Location</th>
</tr>
  <% for designer in @designers %>
  <tr>
    <td><a href="/designers/<%= designer.id %>"><%= designer.name %></a></td>
    <td>|</td>
    <td><%= designer.location %></td>
  </tr>
<% end %>
</table>

</div>






<hr>
